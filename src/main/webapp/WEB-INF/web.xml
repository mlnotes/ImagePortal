<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee        http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>Archetype Created Web Application</display-name>
    <context-param>
        <param-name>config</param-name>
        <param-value>applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>com.mlnotes.web.factory.SimpleContextListener</listener-class>
    </listener>
    <filter>
        <filter-name>route</filter-name>
        <filter-class>com.mlnotes.web.router.RouterFilter</filter-class>
        <init-param>
            <param-name>config</param-name>
            <param-value>struts.xml</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>route</filter-name>
        <url-pattern>*.action</url-pattern>
    </filter-mapping>
    <welcome-file-list>
        <welcome-file>home.action</welcome-file>
    </welcome-file-list>
</web-app>
